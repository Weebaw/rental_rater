.container
  .page-header
    %h1
      Sign In Form

  .well.col-md-3
    =form_tag sign_in_path do |f|

      .form-group
        =label_tag :email
        =email_field_tag :email

      .form-group
        =label_tag :password
        =password_field_tag :password

      .form-group.col-md-offset-4
        =submit_tag "Sign In"
