<!DOCTYPE html>
<html>
<head>
  <title>Rentalrater</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if flash[:notice] %>
  <div class="alert alert-success flash_padding">
    <%= flash[:notice] %>
  </div>

  <% end %>
<%= yield %>

</body>
<footer>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC7tC2Bb5DTVAB9Vdg84umemHqDT9FeKuw&sensor=false"></script>
</footer>
</html>
