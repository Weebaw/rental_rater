.container
  .page-header
    %h1
      Edit Property Form

  =form_for @property do |f|
    =f.label :address
    =f.text_field :address

    =f.label :building_type
    -# =f.collection_select()

    =f.label :building_name
    =f.text_field :building_name

    =f.label :apt_number "Apartment Number"
    =f.text_field :apt_number

    =f.label :city
    =f.text_field :city

    =f.label :landlord
    =f.text_field :landlord

    =f.submit "Edit Property"
